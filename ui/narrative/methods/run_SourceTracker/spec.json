{
  "ver" : "0.0.1",
  "authors" : ["smunzir"],
  "contact" : "http://kbase.us/contact-us/",
  "visible" : true,
  "categories" : ["active","util"],
  "widgets": {
    "input": "kbaseNarrativeMethodInput",
    "output": "no-display"
  },
  "parameters" : [
    {
      "id" : "amplicon_matrix_ref",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : {
        "valid_ws_types" : ["KBaseMatrices.AmpliconMatrix"]
      }
    },
    {
      "id" : "metadata_table",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : {
        "valid_ws_types" : ["KBaseSets.SampleSet"]
      }
    },
    {
      "id": "meta_field",
      "optional": false,
      "advanced": false,
      "allow_multiple": false,
      "default_values": [ "" ],
      "field_type": "textsubdata",
      "textsubdata_options": {
        "subdata_selection": {
           "parameter_id": "metadata_table",
           "path_to_subdata": [
               "row_attributemapping_ref",
               "<WSREF>",
               "attributes"
           ],
           "subdata_included": [
               "attributes/[*]/attribute",
               "attributes/[*]/source",
               "row_attributemapping_ref"
           ],
           "selection_id": "attribute",
           "selection_description": [
               "attribute",
               "source"
           ],
           "description_template": "source: {{source}}"
        },
        "multiselection": false,
        "show_src_obj": true,
        "allow_custom": false
      }
    },
    {
      "id": "threshold",
      "optional": false,
      "advanced": true,
      "allow_multiple": false,
      "default_values": [ "0.005" ],
      "field_type": "text",
      "text_options": {
        "validate_as": "float",
        "min_float" : 0.0,
        "max_float" : 1.0
      }
    },
    {
      "id": "sink_or_sample",
      "optional": true,
      "advanced": true,
      "allow_multiple": false,
      "default_values": [ "" ],
      "field_type": "textsubdata",
      "textsubdata_options": {
        "subdata_selection": {
           "parameter_id": "amplicon_matrix_ref",
           "path_to_subdata": [
               "col_attributemapping_ref",
               "<WSREF>",
               "attributes"
           ],
           "subdata_included": [
               "attributes/[*]/attribute",
               "attributes/[*]/source",
               "col_attributemapping_ref"
           ],
           "selection_id": "attribute",
           "selection_description": [
               "attribute",
               "source"
           ],
           "description_template": "source: {{source}}"
        },
        "multiselection": false,
        "show_src_obj": true,
        "allow_custom": false
      }
    },
    {
      "id" : "associated_matrix_row",
      "optional" : true,
      "advanced" : true,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "textsubdata",
      "textsubdata_options" : {
        "subdata_selection": {
          "parameter_id" : "metadata_table",
          "subdata_included" : ["data/row_ids"],
          "path_to_subdata": ["data", "row_ids"],
          "selection_id": "id"
          },
        "multiselection":false,
        "show_src_obj":false,
        "allow_custom":false
      }
    },
    {
      "id": "ascending",
      "optional":true,
      "advanced":true,
      "allow_multiple":false,
      "default_values":["1"],
      "field_type" : "checkbox",
      "checkbox_options":{
        "checked_value": 1,
        "unchecked_value": 0
      }
    }
  ],
  "behavior" : {
    "service-mapping" : {
      "url" : "",
      "name" : "SourceTracker",
      "method" : "run_SourceTracker",
      "input_mapping" : [
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspace_name"
        },
        {
          "narrative_system_variable": "workspace_id",
          "target_property": "workspace_id"
        },
        {
          "input_parameter" : "amplicon_matrix_ref",
          "target_property" : "amplicon_matrix_ref",
          "target_type_transform": "resolved-ref"
        },
        {
          "input_parameter" : "metadata_table",
          "target_property" : "metadata_table",
          "target_type_transform": "resolved-ref"
        },
        {
          "input_parameter" : "threshold",
          "target_property" : "threshold"
        },
        {
          "input_parameter": "sink_or_sample",
          "target_property": "sink_or_sample"
        },
        {
          "input_parameter": "meta_field",
          "target_property": "meta_field"
        },
        {
          "input_parameter" : "associated_matrix_row",
          "target_property" : "associated_matrix_row"
        },
        {
          "input_parameter" : "ascending",
          "target_property" : "ascending"
        }
      ],
      "output_mapping" : [
        {
            "service_method_output_path": [0, "report_name"],
            "target_property": "report_name"
        },
        {
            "service_method_output_path": [0, "report_ref"],
            "target_property": "report_ref"
        },
        {
            "constant_value": "16",
            "target_property": "report_window_line_height"
        }
      ]
    }
  },
  "job_id_output_field": "docker"
}
